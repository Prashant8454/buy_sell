{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "lat": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "float",
      "required": true
    },
    "lang": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "float",
      "required": true
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "phoneNumber": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal",
      "required": true
    },
    "user_id": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "user_name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "sub_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-category.sub-category",
      "inversedBy": "products"
    }
  }
}
